   function arg(...arguments) {
    return new Promise((resolve, reject) => {
      if (arguments.length > 0) {
        resolve(arguments);
      } else {
        reject('No arguments passed!');
      }
    });
  }

  async function Data() {
    try {
      const result = await arg('n01533954','nisha');
      console.log(result);
    } catch (error) {
      console.error(error);
    }
  }
 
  Data();